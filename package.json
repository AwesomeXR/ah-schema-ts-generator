{
  "name": "ah-schema-ts-generator",
  "version": "1.0.11",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": "dist/cli-tsnode-esm.js",
  "scripts": {
    "test": "jest",
    "ci": "prettier --check src && tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.build.json --outDir dist",
    "release": "npm run ci && rm -rf dist/* && npm run build && release-it"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "ah-api-type": "^3.0.7",
    "ah-logger": "^3.2.5",
    "glob": "^8.0.3",
    "prettier": "^3.2.4",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.8.5",
    "@types/yargs": "^17.0.9",
    "jest": "^29.7.0",
    "nodemon": "^2.0.5",
    "release-it": "^17.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
